        output="screen"
    /> -->

    <node
        pkg="ars548_driver"
        type="radar_project.py"
        name="radar_project"
        output="screen"
    />

    <!-- <node
        pkg="ars548_driver"
        type="keyboard.py"
        name="keyboard"
        output="screen"
    /> -->

    <!-- camera only -->
    <!-- <node
        pkg="ars548_driver"
